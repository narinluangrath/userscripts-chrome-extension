{
  "manifest_version": 2,
  "name": "Userscripts Chrome Extension",
  "version": "0.0.1",
  "description": "Userscript manager",
  "browser_action": {
    "default_icon": {
      "16": "icons/secondary-icon-16.png",
      "32": "icons/secondary-icon-32.png",
      "64": "icons/secondary-icon-64.png",
      "128": "icons/secondary-icon-128.png"
    },
    "default_title": "User Scripts",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/secondary-icon-16.png",
    "32": "icons/secondary-icon-32.png",
    "64": "icons/secondary-icon-64.png",
    "128": "icons/secondary-icon-128.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": ["activeTab", "storage", "tabs", "*://*/*", "webNavigation"],
  "options_page": "options.html"
}
